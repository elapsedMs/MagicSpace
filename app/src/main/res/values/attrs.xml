<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HomeTabView">
        <attr name="home_tab_style" format="integer">
            <enum name="album" value="1" />
            <enum name="egg" value="2" />
            <enum name="my" value="3" />
            <enum name="setting" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AlbumTitleView">
        <attr name="album_title_style" format="integer">
            <enum name="normal" value="1" />
            <enum name="have_count" value="2" />
        </attr>
        <attr name="album_title_name" format="string" />
    </declare-styleable>


</resources>